{
  "name": "playwright_automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:asset": "playwright test tests/assets/asset-register.test.ts --headed --project=chromium",
    "test:asset:smoke": "playwright test tests/asset-register.test.ts --grep \"(?=.*@smoke)(?=.*@feature-asset)\" --headed --project=chromium",
    "test:report": "playwright show-report",
    "install:browsers": "playwright install --with-deps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.10.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "mssql": "^11.0.1",
    "playwright-extra": "^4.3.6",
    "playwright-extra-plugin-stealth": "^0.0.1"
  }
}
