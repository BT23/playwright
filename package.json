{
  "name": "playwright_automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "@playwright/test": "^1.49.0",
    "test:features": "playwright test --grep \"@feature-(wo|asset|request|supplier|po)\" --project=chromium --reporter=html",
    "test:requests": "playwright test tests/requests --project=chromium --reporter=html",
    "test:report": "playwright show-report",
    "install:browsers": "playwright install --with-deps",

    "test:smoke": "playwright test --grep \"@smoke\" --project=chromium --reporter=html"
    
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.10.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "mssql": "^11.0.1",
    "playwright-extra": "^4.3.6",
    "playwright-extra-plugin-stealth": "^0.0.1"
  }
}
