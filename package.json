{
  "name": "playwright_automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:smoke": "playwright test tests --grep '@smoke' --project=chromium --reporter=list",
    "test:full": "playwright test tests --project=chromium --reporter=list",
    "test:feature-asset": "playwright test tests --grep '(?=.*@smoke)(?=.*@feature-asset)' --project=chromium --reporter=html",
    "test:report": "playwright show-report",
    "install:browsers": "playwright install --with-deps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.10.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "mssql": "^11.0.1",
    "playwright-extra": "^4.3.6",
    "playwright-extra-plugin-stealth": "^0.0.1"
  }
}
